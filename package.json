{
  "name": "democrat",
  "version": "3.1.1",
  "description": "React, but for state management !",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/democrat#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/democrat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/democrat.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/democrat.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "codecov": "codecov",
    "lint": "tsdx lint src test",
    "pub": "yarn build && np",
    "start": "tsdx watch",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watch",
    "example:run": "esbuild example/index.tsx --servedir=example --outdir=example --bundle --define:process.env.NODE_ENV=\"'development'\""
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "999.999.999"
      }
    },
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-redeclare": 0
    }
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "dependencies": {
    "suub": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "codecov": "^3.8.1",
    "esbuild": "^0.9.0",
    "immer": "^8.0.1",
    "np": "^7.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
