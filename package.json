{
  "name": "democrat",
  "version": "3.0.4",
  "description": "React, but for state management !",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/democrat#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/democrat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/democrat.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/democrat.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "postinstall": "husky install",
    "codecov": "codecov",
    "lint": "tsdx lint src test",
    "pub": "yarn build && np",
    "start": "tsdx watch",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "suub": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "codecov": "^3.8.1",
    "husky": "^5.1.3",
    "np": "^7.4.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "version": "999.999.999"
      }
    },
    "rules": {
      "no-redeclare": 0
    }
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
